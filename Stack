//follow the LIFO principle using dynamic array or linked list
public class Stack<Stype>{
private DynamicArray<Stype>topStack
private int size;
}
public Stack(){
topStack=new DynamicArray<Stype>();
size= new size;
}
public Push(Stype item){
topStack.Add(item); //adding item
}
public Pop(){
if(topStack == -1){
    print("Empty");
}
return topStack.Remove(topStack.size()-1); //returning removed item back
}
public Peak(){
if(topStack == -1){
    print("Empty");
}
    return topStack.Remove(topStack.size()-1); //retunring item back
}
public Size(){
return topStack.Size(); //current number of items
}
